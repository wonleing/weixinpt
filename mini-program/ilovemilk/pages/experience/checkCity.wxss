/* checkCity.wxss */
.containers{
  width: 694.6rpx;
  height: 100%;
  padding-right: 59.78rpx;
  overflow-y: scroll;
}
.city{
  color: #a3a3a3;
  font-size: 21rpx;
  margin-top: 33.82rpx;
   padding-left: 59.78rpx;
}
.city .cityName{
  color: #000;
  font-size: 33.82rpx;
  line-height: 65.22rpx;
}
.cityList{
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
}
.cityList .cityType{
  width: 50%;
  display: flex;
  flex-direction: column;
  text-align: center;
  margin-bottom: 59.98rpx;
  position: relative;
}
.cityList .cityType .logo{
  width: 173.91rpx;
  height: 218rpx;
  margin: 24.15rpx auto;
}
.cityList .cityType .logo image{
  width: 100%;
  height: 100%;
}

.cityList .logo+view{
  white-space: nowrap;
  font-size: 26rpx;
}
.checked{
  color: #1cac1b;
  font-size: 36rpx;
  position: absolute;
  right: 20rpx;
  bottom: 0;
}
.nochecked{
  display: none;
}